<!doctype html>
<html lang="en">
    <head>
        <meta content="text/html; charset=UTF-8" http-equiv="content-type"/>
        {% if title %}
            <title>{{ title }}</title>
        {% else %}
            <title>Finance Analysis</title>
        {% endif %}
        {% if stylesheet %}
